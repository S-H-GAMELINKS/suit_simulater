<header>
  <h1>
    <%= link_to "Suit simulater", root_path %>
  </h1>
  <nav class="pc-nav">
    <ul>
      <li><%= link_to "find suit", suit_path %></li>
      <li><%= link_to "find shirt", shirt_path %></li>
      <li><%= link_to "find tie", tie_path %></li>
      <% if logged_in? %>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">マイアカウント <b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li><%= link_to "マイページ", current_user %></li>
            <li><%= link_to "設定", '#' %></li>
            <li class="divider"></li>
            <li><%= link_to "ログアウト", logout_path, method: :delete %></li>
          </ul>
        </li>
      <% else %>
        <li><%= link_to "Log in", login_path %></li>
      <% end %>
    </ul>
  </nav>

</header>
